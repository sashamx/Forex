    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Work</name>
    <description>
        Forex signal.
    </description>
    <author email="sashamx@gmail.com" href="http://khamdamov.ru">
        Khamdamov Aleksander
    </author>
    <content src="index.html" />
    <access origin="*" />
